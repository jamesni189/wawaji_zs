<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ydd.pockettoycatcherrwz.ui.home.WaWaBoxActivity">

    <RelativeLayout
        android:background="@color/app_login_title_color"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:id="@+id/title_back"
            android:src="@mipmap/ic_title_back"
            android:padding="15dp"
            android:layout_width="50dp"
            android:layout_height="50dp" />

        <TextView
            android:id="@+id/btn_manito"
            android:text="娃娃盒"
            android:textColor="@color/white"
            android:gravity="center"
            android:textSize="16dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>
    <com.github.lzyzsd.jsbridge.BridgeWebView
        android:id="@+id/bwView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.github.lzyzsd.jsbridge.BridgeWebView>


</LinearLayout>
